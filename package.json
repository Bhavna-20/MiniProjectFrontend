{
  "name": "brightreturn-web-ui",
  "version": "1.2.4",
  "scripts": {
    "dev": "vite --force --host",
    "build": "run-p type-check build-only",
    "preview": "pnpm build-only && vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier ./src -write",
    "prepare": "husky install",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "mobile:build": "VITE_API_ENDPOINT=https://app.brightreturn.com/api/v1 pnpm build && pnpm cap sync && pnpm cap copy"
  },
  "dependencies": {
    "@fullcalendar/core": "6.1.8",
    "@fullcalendar/daygrid": "6.1.8",
    "@fullcalendar/interaction": "6.1.8",
    "@fullcalendar/vue3": "6.1.8",
    "@nangohq/frontend": "^0.25.8",
    "@novu/notification-center-vue": "^0.16.3",
    "@types/papaparse": "5.3.7",
    "@vueuse/core": "10.3.0",
    "@vueuse/head": "1.1.26",
    "@vueuse/integrations": "10.3.0",
    "@vueuse/router": "10.3.0",
    "awesome-phonenumber": "5.10.0",
    "axios": "1.4.0",
    "chart.js": "4.3.0",
    "dayjs": "1.11.9",
    "dayjs-business-days2": "1.2.1",
    "floating-vue": "2.0.0-beta.24",
    "nprogress": "0.2.0",
    "papaparse": "5.4.1",
    "pretty-bytes": "6.1.0",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primevue": "3.31.0",
    "quill": "1.3.7",
    "universal-cookie": "4.0.4",
    "unplugin-vue-components": "0.25.1",
    "vee-validate": "4.6.7",
    "vite-plugin-pages": "0.31.0",
    "vite-plugin-vue-layouts": "0.8.0",
    "vue": "3.3.4",
    "vue-query": "1.26.0",
    "vue-router": "4.2.4",
    "vue3-scroll-spy": "1.0.8",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@capacitor/android": "5.1.1",
    "@capacitor/app": "5.0.5",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "5.1.1",
    "@capacitor/core": "5.1.1",
    "@capacitor/haptics": "5.0.5",
    "@capacitor/ios": "5.1.1",
    "@capacitor/keyboard": "5.0.5",
    "@capacitor/splash-screen": "5.0.5",
    "@capacitor/status-bar": "5.0.5",
    "@iconify/vue": "4.1.1",
    "@rushstack/eslint-patch": "1.3.2",
    "@types/node": "20.4.1",
    "@types/papaparse": "5.3.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/tsconfig": "0.4.0",
    "add": "2.0.6",
    "commit-and-tag-version": "11.2.2",
    "cordova-res": "0.15.4",
    "eslint": "8.44.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.15.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "sass": "1.63.6",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "unocss": "0.53.5",
    "unplugin-auto-import": "0.16.6",
    "vite": "4.4.9",
    "vue-tsc": "1.8.8"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --fix",
    "*.{js,css,md,ts,vue}": "prettier --write"
  },
  "engines": {
    "node": "18.x"
  },
  "resolutions": {
    "eslint-plugin-prettier": "5.0.0"
  }
}
